    eprint = {http://amstat.tandfonline.com/doi/pdf/10.1080/10618600.2012.681216},
    journal = {J Comp Graph Stat},
    keywords = {bootstrap, cluster-bootstrap, clustered-data, longitudinal, serial, *import},
    number = {2},
