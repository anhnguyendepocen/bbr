    citeulike-linkout-1 = {http://www.hubmed.org/display.cgi?uids=25633190},
    day = {15},
    issn = {1879-1913},
    journal = {The American journal of cardiology},
    keywords = {clinical-prediction-rule, collaboration, emergency-medicine-research},
